package tests;

public class calculator {
	
	public int addnum(int i , int j)
	{
		return i+j;
	}
	
	public int subnum(int i , int j)
	{
		return i-j;
	}
	
	public int mulnum(int i , int j)
	{
		return i*j;
	}
	
	public int divnum(int i , int j)
	{
		return i/j;
	}
	
	public int modnum(int i , int j)
	{
		return i%j;
	}


}
